{
  "manifest_version": 3,
  "name": "Auto Meta - Automation for Meta AI",
  "version": "1.0.0",
  "description": "Workflow automation for Meta AI - batch process prompts and download results",
  "action": {
    "default_title": "Auto Meta"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/index.ts-kR8FnpO4.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "sidePanel"
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/index.ts-kR8FnpO4.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
